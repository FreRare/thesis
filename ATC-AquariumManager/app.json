{
  "expo": {
    "name": "ATC-AquariumManager",
    "slug": "atc-aquariummanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/ATC_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/ATC_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frerare.atcaquariummanager"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/ATC_logo.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.frerare.atcaquariummanager",
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "eas": {
        "projectId": "9d33dc99-5dc7-4cbf-9fa8-8b351cd1b878"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "owner": "frerare"
  }
}
